{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "check-types": "tsc --noEmit",
    "test": "yarn run lint",
    "pretty": "prettier --write \"src/**/*.{ts,tsx}\"",
    "lint": "yarn run check-types && tslint -p tsconfig.json -c tslint.json \"src/**/*.{ts,tsx}\"",
    "lint:fix": "yarn run check-types && tslint -p tsconfig.json -c tslint.json --fix \"src/**/*.{ts,tsx}\""
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,json,css}": [
      "yarn pretty",
      "yarn lint:fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "printWidth": 120,
    "parser": "typescript"
  },
  "dependencies": {
    "@wobbly/api-client": "^0.0.1-alpha.8",
    "expo": "^32.0.0",
    "formik": "^1.4.3",
    "lodash": "^4.17.11",
    "react": "16.5.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-32.0.0.tar.gz",
    "react-navigation": "^3.0.9",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "tslib": "^1.9.3",
    "typesafe-actions": "^3.0.0",
    "yup": "^0.26.10"
  },
  "devDependencies": {
    "@types/expo": "^31.0.9",
    "@types/expo__vector-icons": "^9.0.0",
    "@types/jest": "^23.3.13",
    "@types/lodash": "^4.14.120",
    "@types/react": "^16.7.22",
    "@types/react-dom": "^16.0.11",
    "@types/react-native": "^0.57.32",
    "@types/react-navigation": "^3.0.1",
    "@types/react-redux": "^7.0.1",
    "@types/react-test-renderer": "^16.0.3",
    "@types/yup": "^0.26.7",
    "babel-preset-expo": "^5.0.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.1",
    "prettier": "^1.16.3",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.1"
  },
  "private": true
}
